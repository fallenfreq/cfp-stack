{
	"name": "somefreq-app",
	"version": "0.0.0",
	"private": true,
	"type": "module",
	"scripts": {
		"dev": "pnpm --filter @somefreq-app/* --parallel dev",
		"build": "pnpm --filter @somefreq-app/* --parallel build",
		"dev:client": "pnpm --filter @somefreq-app/client dev",
		"dev:vite:client": "pnpm --filter @somefreq-app/client dev:vite",
		"dev:api": "pnpm --filter @somefreq-app/api dev",
		"build:client": "pnpm --filter @somefreq-app/client build",
		"build:api": "pnpm --filter @somefreq-app/api build",
		"preview:client": "pnpm --filter @somefreq-app/client preview",
		"lint": "eslint --fix",
		"format": "prettier --write",
		"migrate:api": "pnpm --filter @somefreq-app/api migrate",
		"migrate:custom": "pnpm --filter @somefreq-app/api migrate:custom",
		"migrate:push:api": "pnpm --filter @somefreq-app/api migrate:push",
		"migrate:push:local:api": "pnpm --filter @somefreq-app/api migrate:push:local",
		"logging:api": "pnpm --filter @somefreq-app/api logging"
	},
	"dependencies": {
		"axios": "^1.7.2",
		"jwt-decode": "^4.0.0",
		"lodash": "^4.17.21",
		"superjson": "2.2.1"
	},
	"devDependencies": {
		"@eslint/js": "^9.19.0",
		"@eslint/json": "^0.12.0",
		"@eslint/markdown": "^6.4.0",
		"@stylistic/eslint-plugin": "^4.4.0",
		"@tsconfig/node20": "^20.1.5",
		"@types/lodash": "^4.17.0",
		"@types/node": "^20.12.5",
		"concurrently": "^8.2.2",
		"eslint": "^9.27.0",
		"globals": "^16.2.0",
		"json": "^11.0.0",
		"markdown": "^0.5.0",
		"npm-run-all2": "^6.1.2",
		"prettier": "^3.2.5",
		"prettier-plugin-organize-imports": "^4.1.0",
		"stylistic": "^0.2.4",
		"typescript": "~5.4.0",
		"typescript-eslint": "^8.23.0"
	},
	"packageManager": "pnpm@10.4.1+sha512.c753b6c3ad7afa13af388fa6d808035a008e30ea9993f58c6663e2bc5ff21679aa834db094987129aa4d488b86df57f7b634981b2f827cdcacc698cc0cfb88af"
}
