{
	"name": "somefreq-app",
	"version": "0.0.0",
	"private": true,
	"type": "module",
	"engines": {},
	"devEngines": {
		"packageManager": {
			"name": "pnpm",
			"version": "10.14.0",
			"onFail": "error"
		},
		"runtime": {
			"name": "node",
			"version": "^24",
			"onFail": "error"
		}
	},
	"scripts": {
		"dev": "pnpm --filter @somefreq-app/* --parallel dev",
		"build": "pnpm --filter @somefreq-app/* --parallel build",
		"dev:client": "pnpm --filter @somefreq-app/client dev",
		"dev:vite:client": "pnpm --filter @somefreq-app/client dev:vite",
		"dev:api": "pnpm --filter @somefreq-app/api dev",
		"build:client": "pnpm --filter @somefreq-app/client build",
		"build:api": "pnpm --filter @somefreq-app/api build",
		"preview:client": "pnpm --filter @somefreq-app/client preview",
		"lint": "eslint --flag unstable_config_lookup_from_file --fix",
		"format": "prettier --write .",
		"migrate:api": "pnpm --filter @somefreq-app/api migrate",
		"migrate:custom": "pnpm --filter @somefreq-app/api migrate:custom",
		"migrate:push:api": "pnpm --filter @somefreq-app/api migrate:push",
		"migrate:push:local:api": "pnpm --filter @somefreq-app/api migrate:push:local",
		"logging:api": "pnpm --filter @somefreq-app/api logging"
	},
	"dependencies": {
		"axios": "^1.9.0",
		"jwt-decode": "^4.0.0",
		"lodash": "^4.17.21",
		"superjson": "2.2.1"
	},
	"devDependencies": {
		"@eslint/js": "^9.28.0",
		"@eslint/json": "^0.12.0",
		"@eslint/markdown": "^6.4.0",
		"@stylistic/eslint-plugin": "^4.4.0",
		"@tsconfig/node20": "^20.1.5",
		"@types/lodash": "^4.17.17",
		"@types/node": "^20.17.57",
		"concurrently": "^8.2.2",
		"eslint": "^9.28.0",
		"eslint-plugin-vue": "^9.33.0",
		"globals": "^16.2.0",
		"json": "^11.0.0",
		"markdown": "^0.5.0",
		"npm-run-all2": "^6.2.6",
		"prettier": "^3.5.3",
		"prettier-plugin-multiline-arrays": "^4.0.3",
		"prettier-plugin-organize-imports": "^4.1.0",
		"stylistic": "^0.2.4",
		"typescript": "~5.4.5",
		"typescript-eslint": "^8.33.0"
	},
	"packageManager": "pnpm@10.14.0+sha512.ad27a79641b49c3e481a16a805baa71817a04bbe06a38d17e60e2eaee83f6a146c6a688125f5792e48dd5ba30e7da52a5cda4c3992b9ccf333f9ce223af84748"
}
